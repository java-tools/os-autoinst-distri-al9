                "QEMURAM": "2048",
                "QEMUVGA": "virtio",
                "QEMU_VIRTIO_RNG": "1",
                "UEFI": "1",
                "UEFI_PFLASH_CODE": "/usr/share/edk2/ovmf/OVMF_CODE.fd",
                "UEFI_PFLASH_VARS": "/usr/share/edk2/ovmf/OVMF_VARS.fd",
                "WORKER_CLASS": "qemu_x86_64"
            }
        }
